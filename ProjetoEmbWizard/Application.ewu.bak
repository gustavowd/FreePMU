  $rect <500,0,700,40>
  method string float2String( arg float arg1 )
  {
    var string valor = "";
    var string valor2;
    var int32 inteiro;
    var float dec;

    if (arg1 < 0.0) {
      valor = "-";
      inteiro = (int32)(-arg1);
      dec = (arg1 + (float)inteiro)*-1000.0;
    } else {
      inteiro = (int32)arg1;
      dec = (arg1 - (float)inteiro)*1000.0;
    }

    valor = valor + string(inteiro);
    valor2 = string((int32)dec, 3);
    return (valor + "," + valor2);
  }

