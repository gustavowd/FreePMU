  $rect <200,290,380,330>
  slot plotar
  {
    var float verde;
    var float amarelo;
    var float vermelho;

    verde = device.getMagR();
    amarelo = device.getMagS();
    vermelho = device.getMagT();

    Rval_verde.String = float2String(verde);
    Sval_amarelo.String = float2String(amarelo);
    Tval_vermelho.String = float2String(vermelho);

    verde = verde - valorMeio;
    amarelo = amarelo - valorMeio;
    vermelho = vermelho - valorMeio;

    if (!troca) {  
      dados_verde.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Rgraf_fverde.PixelPerUnit.x, -verde);
      dadosmirror_verde.AddCoord(-JanelaG.ScrollOffset.x/Rgraf_fverde.PixelPerUnit.x, -verde);

      dados_amarelo.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Sgraf_famarelo.PixelPerUnit.x, -amarelo);
      dadosmirror_amarelo.AddCoord(-JanelaG.ScrollOffset.x/Sgraf_famarelo.PixelPerUnit.x, -amarelo);

      dados_vermelho.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Tgraf_fvermelho.PixelPerUnit.x, -vermelho);
      dadosmirror_vermelho.AddCoord(-JanelaG.ScrollOffset.x/Tgraf_fvermelho.PixelPerUnit.x, -vermelho);

      JanelaG.ScrollOffset.x = JanelaG.ScrollOffset.x - Rgraf_fverde.PixelPerUnit.x;
      Rgraf_fverde.Coordinates = dados_verde;
      Sgraf_famarelo.Coordinates = dados_amarelo;
      Tgraf_fvermelho.Coordinates = dados_vermelho;
      
      if ((0-JanelaG.ScrollOffset.x) == JanelaG.Bounds.w) {
        Rgraf_fverde.Coordinates = dadosmirror_verde;
        dados_verde.ClearList();

        Sgraf_famarelo.Coordinates = dadosmirror_amarelo;
        dados_amarelo.ClearList();

        Tgraf_fvermelho.Coordinates = dadosmirror_vermelho;
        dados_vermelho.ClearList();

        JanelaG.ScrollOffset.x = 0;
        troca = true;
      }
    } else {
      dadosmirror_verde.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Rgraf_fverde.PixelPerUnit.x, -verde);
      dados_verde.AddCoord( -JanelaG.ScrollOffset.x/Rgraf_fverde.PixelPerUnit.x, -verde);

      dadosmirror_amarelo.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Sgraf_famarelo.PixelPerUnit.x, -amarelo);
      dados_amarelo.AddCoord( -JanelaG.ScrollOffset.x/Sgraf_famarelo.PixelPerUnit.x, -amarelo);

      dadosmirror_vermelho.AddCoord( (-JanelaG.ScrollOffset.x + JanelaG.Bounds.w)/Tgraf_fvermelho.PixelPerUnit.x, -vermelho);
      dados_vermelho.AddCoord( -JanelaG.ScrollOffset.x/Tgraf_fvermelho.PixelPerUnit.x, -vermelho);

      JanelaG.ScrollOffset.x = JanelaG.ScrollOffset.x - Rgraf_fverde.PixelPerUnit.x;
      Rgraf_fverde.Coordinates = dadosmirror_verde;
      Sgraf_famarelo.Coordinates = dadosmirror_amarelo;
      Tgraf_fvermelho.Coordinates = dadosmirror_vermelho;
      
      if ((0-JanelaG.ScrollOffset.x) == JanelaG.Bounds.w) {
        Rgraf_fverde.Coordinates = dados_verde;
        dadosmirror_verde.ClearList();

        Sgraf_famarelo.Coordinates = dados_amarelo;
        dadosmirror_amarelo.ClearList();

        Tgraf_fvermelho.Coordinates = dados_vermelho;
        dadosmirror_vermelho.ClearList();

        JanelaG.ScrollOffset.x = 0;
        troca = false;
      }
    }
  }

  $rect <500,0,700,40>
  method string float2String( arg float arg1 )
  {
    var string valor = "";
    var string valor2;
    var int32 inteiro;
    var float dec;

    if (arg1 < 0.0) {
      valor = "-";
      inteiro = (int32)(-arg1);
      dec = (arg1 + (float)inteiro)*-10.0;
    } else {
      inteiro = (int32)arg1;
      dec = (arg1 - (float)inteiro)*10.0;
    }

    valor = valor + string(inteiro);
    valor2 = string((int32)dec, 1);
    return (valor + "," + valor2);
  }

  $rect <720,270,900,310>
