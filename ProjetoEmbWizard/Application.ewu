    val_t_1.String = string(int32( float(JanelaG.Bounds.w/4)   * (float(intervalo.Period)/1000.0) / float(grafico.PixelPerUnit.x) )) + "s";
    val_t_2.String = string(int32( float(JanelaG.Bounds.w/2)   * (float(intervalo.Period)/1000.0) / float(grafico.PixelPerUnit.x) )) + "s";
    val_t_3.String = string(int32( float(3*JanelaG.Bounds.w/4) * (float(intervalo.Period)/1000.0) / float(grafico.PixelPerUnit.x) )) + "s";
    val_t_4.String = string(int32( float(JanelaG.Bounds.w)     * (float(intervalo.Period)/1000.0) / float(grafico.PixelPerUnit.x) )) + "s";

    /* Cálculo dos rótulos dos valores de frequência, com base na proporção de pixels por unidade de medida  do
       eixo y (PixelPerUnit.y) do objeto Charts::Graph "grafico". */
    val_fr_1.String = float2String(valorMeio) + " Hz";                        
    val_fr_0.String = float2String(valorMeio - float(grafico.Bounds.h/3)/float(grafico.PixelPerUnit.y)) + " Hz";
