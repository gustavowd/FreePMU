  slot atualizaFFT
  {
    if (faseAtual == 0) {
      graf_FFT.Coordinates = device.getFFT_R();
    } else if (faseAtual == 1) {
      graf_FFT.Coordinates = device.getFFT_S();  
    } else if (faseAtual == 2) {
      graf_FFT.Coordinates = device.getFFT_T();
    }
  }

